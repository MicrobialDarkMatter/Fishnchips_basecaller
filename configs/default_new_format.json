{
    "model":{
        "signal_window_size": 300,
        "label_window_size": 100,
        "attention_blocks": 4,
        "cnn_blocks": 5,
        "maxpool_idx": 3,
        "d_model": 250,
        "dff": 512,
        "num_heads": 25,
        "dropout_rate": 0.1,
        "maxpool_kernel": 4
    },
    "training":{
        "data":"./data/ds1_labelled_segmented_fnch/train.hdf5",
        "epochs": 3,
        "warmup":100, 
        "patience": 300,
        "batch_size":300,
        "buffer_size":5,
        "lr_mult": 1
    },
    "validation":{
        "data":"./data/ds1_labelled_segmented_fnch/validate.hdf5",
        "batch_size":32,
        "algorithm": "editdistance",
        "buffer_size":5,
        "reads":3
    },
    "testing":{
        "data":"./data/ds1_labelled_segmented_fnch",
        "batch_size":300,
        "buffer_size":5
    }
}
